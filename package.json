{
  "name": "reactable",
  "version": "0.1.3",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "luxon": "^1.8.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.1",
    "react-flatpickr": "^3.10.6",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "xss": "^1.0.6"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.(jsx|js)?$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/tests/fileMock.js"
    }
  },
  "scripts": {
    "build:dev": "webpack --mode=development --watch --config config/webpack.dev.js",
    "start:dev": "webpack-dev-server --mode=development --watch --config config/webpack.dev.js  --host 0.0.0.0",
    "develop": "webpack-dev-server --mode=development --watch --config config/webpack.dev.js  --host 0.0.0.0",
    "build:prod": "webpack --mode=production --config config/webpack.prod.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "base64-inline-loader": "^1.1.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.4.0",
    "html-loader": "^0.5.5",
    "jest": "^24.8.0",
    "prettier": "^2.2.1",
    "serve": "^10.1.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
